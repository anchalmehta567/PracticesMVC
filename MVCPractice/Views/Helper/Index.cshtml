@model IEnumerable<MVCPractice.Models.Employees>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>@Html.DisplayNameFor(x => x.Id)</th>
        <th>@Html.DisplayNameFor(x => x.Name)</th>
        <th>@Html.DisplayNameFor(x => x.Address)</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(Item => item.Id)</td>
            <td>@Html.DisplayFor(Item => item.Name)</td>
            <td>@Html.DisplayFor(Item => item.Address)</td>
        </tr>
    }
</table>